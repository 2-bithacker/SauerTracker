    "bluebird": "^3.2.1",
    "body-parser": "^1.14.2",
    "compression": "^1.5.2",
    "express": "^4.12.3",
    "forever": "^0.15.1",
    "fuzzaldrin": "^2.1.0",
    "geoip-lite": "1.x",
    "hbs": "^3.0.1",
    "i18n-iso-countries": "^0.9.0",
    "internet-relay-chat": "^1.1.5",
    "jade": "^1.11.0",
    "knex": "^0.9.0",
    "lodash": "^4.0.0",
    "moment": "^2.10.6",
    "nodemailer": "^1.4.0",
    "pg": "^4.4.4",
