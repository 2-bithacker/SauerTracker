{
  "name": "sauer-tracker",
  "version": "2.3.0",
  "description": "Server and stats tracker for Cube 2: Sauerbraten.",
  "main": "main.js",
  "repository": "https://github.com/AngrySnout/SauerTracker",
  "scripts": {
    "postinstall": "cd website && npm install && bower install",
    "build": "NODE_ENV=production knex migrate:latest && NODE_ENV=production gulp build && cd website && NODE_ENV=production gulp build",
    "stop": "NODE_ENV=production forever stop sauertracker",
    "start": "NODE_ENV=production forever -a --uid \"sauertracker\" start --killSignal=SIGINT main.js",
    "bumpv": "npm version patch"
  },
  "browser": {
    "jquery": "./bower_components/jquery/dist/jquery.js",
    "foundation": "./bower_components/foundation-sites/js/foundation.js"
  },
  "browserify-shim": {
    "jquery": "$",
    "foundation": "foundation"
  },
  "author": "AngrySnout <hreudontdy@gmail.com>",
  "license": "GPL-3.0",
  "dependencies": {
    "bluebird": "^3.3.5",
    "body-parser": "^1.15.0",
    "compression": "^1.6.1",
    "esdoc": "^0.5.2",
    "express": "^4.13.4",
    "forever": "^0.15.1",
    "fuzzaldrin": "^2.1.0",
    "geoip-lite": "1.x",
    "i18n-iso-countries": "^1.8.0",
    "internet-relay-chat": "git+https://github.com/AngrySnout/node-internet-relay-chat.git",
    "knex": "^0.12.6",
    "lodash": "^4.11.2",
    "moment": "^2.13.0",
    "pg": "^6.1.2",
    "pug": "^2.0.0-beta6",
    "redis": "^2.8.0",
    "response-time": "^2.3.1",
    "source-map-support": "^0.4.0"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.4.1",
    "babelify": "^7.3.0",
    "bower": "^1.7.9",
    "browserify": "^13.0.0",
    "browserify-shim": "^3.8.12",
    "debowerify": "^1.4.1",
    "del": "^2.2.0",
    "esdoc": "^0.4.7",
    "fuzzaldrin": "^2.1.0",
    "glob": "^7.0.3",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^3.1.0",
    "gulp-babel": "^6.1.2",
    "gulp-clean-css": "^2.0.7",
    "gulp-if": "^2.0.0",
    "gulp-plumber": "^1.1.0",
    "gulp-rename": "^1.2.2",
    "gulp-rimraf": "^0.2.0",
    "gulp-sass": "^3.0.0",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-uglify": "^2.0.0",
    "gulp-util": "^3.0.7",
    "gulp-watch": "^4.3.5",
    "jadeify": "^4.6.0",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0",
    "watchify": "^3.7.0"
  }
}
